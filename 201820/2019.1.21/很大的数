#include<stdio.h>
#include<string.h>
int main()
{
    char s1[1000],s2[1000];
    int a[31],b[31],len1,len2,j;
    memset(s1,0,sizeof(s1));
    memset(s2,0,sizeof(s2));
    scanf("%s",s1);
    scanf("%s",s2);
    len1=strlen(s1);
    len2=strlen(s2);
    int i;
    for(i=len1-1,j=0;i>=0;i--)
    {
    	a[j]=s1[i]-48;
    	j++;
	}
	for(i=len2-1,j=0;i>=0;i--)
    {
    	b[i]=s2[j]-48;
    	j++;
	}
	for(i=0;i<(len1<len2?len2:len1);i++)
	{
		a[i]=a[i]+b[i];
		if(a[i]>9)
           {
               a[i+1]+=1;	
              a[i]-=10;
           }
	}
	if(a[i])
        for(j=i;j>-1;j--)
        printf("%d",a[j]);
       else for(j=i-1;j>-1;j--)
        printf("%d",a[j]);
	return 0;
 } 
